<Page x:Class="WpfMedLabApp.Views.Pages.AdminPages.AdminMainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfMedLabApp.Views.Pages.AdminPages"
      mc:Ignorable="d" 
      FontFamily="Comic Sans MS"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminMainPage">

    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Создать отчет" Name="ReportCreateBtn" Click="ReportCreateBtn_Click" Width="200"></Button>
            <Button Content="История входа" Name="LoginedHistory" Click="LoginedHistory_Click" Width="200" Margin="5"></Button>
            <Button Content="Материалы" Name="MaterialsBtn" Click="MaterialsBtn_Click" Width="200"></Button>
        </StackPanel>
    </Grid>
</Page>
